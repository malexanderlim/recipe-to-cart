# Upstash Redis Environment Variables (Get from Upstash Dashboard)
# See: https://upstash.com/docs/redis/overall/connect
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Vercel Blob Storage Environment Variables (Get from Vercel Dashboard)
# See: https://vercel.com/docs/storage/vercel-blob/quickstart#set-up-environment-variables
BLOB_READ_WRITE_TOKEN=

# Google Cloud Vision API Credentials (Paste JSON key content as a single line string)
# See: https://cloud.google.com/vision/docs/setup#create-a-service-account-key
GOOGLE_APPLICATION_CREDENTIALS='{"type": "service_account", ...}'

# Anthropic API Key
# See: https://console.anthropic.com/settings/keys
ANTHROPIC_API_KEY=

# Internal Trigger Secret (Create a random string for basic security)
INTERNAL_TRIGGER_SECRET=your-random-secret-string-here 